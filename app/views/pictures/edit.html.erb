<%= simple_form_for @picture do |f| %>
  <%= f.input :title %>
  <%= f.input :desc %> 
  <%= f.input :photo, as: :file %>  <!-- [...] -->
  <%# <%= f.collection_check_boxes :product_ids, Product.all, :id, :title %>
  <%= f.association :products,as: :check_boxes, 
                              collection: Product.all.map { |product| "#{product.id} / #{product.title}" } %>
    
  <%= f.button :submit, "Update", class: 'btn btn-primary'%>
<% end %>

